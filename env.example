# Configuration de l'application UqarLife
# Copiez ce fichier vers .env et modifiez les valeurs selon votre environnement

# Nom de l'application
VITE_APP_NAME=BourseAnalyse

# URL de l'API backend (si applicable)
VITE_API_URL=http://localhost:8000/api

# URL de l'API de données boursières
VITE_STOCK_API_URL=https://api.example.com/stocks

# Clé API pour les données boursières (si nécessaire)
VITE_STOCK_API_KEY=your_api_key_here

# Mode de l'application
VITE_APP_MODE=production

# URL de base de l'application
VITE_BASE_URL=https://bourseanalyse.vercel.app

# Configuration des analytics (optionnel)
VITE_ANALYTICS_ID=your_analytics_id

# Configuration des fonctionnalités
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REAL_TIME_DATA=true 